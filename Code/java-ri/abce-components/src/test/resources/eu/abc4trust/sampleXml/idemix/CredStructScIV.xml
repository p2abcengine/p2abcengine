<?xml version="1.0" encoding="UTF-8"?>
<CredentialStructure xmlns="http://www.zurich.ibm.com/security/idemix/credentialStructure"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xsi:schemaLocation="http://www.zurich.ibm.com/security/idemix/credentialStructure ../../idemix/CredentialStructure.xsd">
    
    <Attributes>
        <Attribute issuanceMode="known" name="attr1" type="int" />
        <Attribute issuanceMode="known" name="attr2" type="int" />
        <Attribute issuanceMode="hidden" name="attr3" type="int" />
        <Attribute issuanceMode="hidden" name="attr4" type="int" />
        <Attribute issuanceMode="issuer" name="attr5" type="int" />
        <Attribute issuanceMode="issuer" name="attr6" type="int" />
    </Attributes>

    <Features>
      <OnSmartcard>true</OnSmartcard>
    </Features>

    <Implementation>
        <AttributeOrder>
            <Attribute name="attr1">1</Attribute>
            <Attribute name="attr2">2</Attribute>
            <Attribute name="attr3">3</Attribute>
            <Attribute name="attr4">4</Attribute>
            <Attribute name="attr5">5</Attribute>
            <Attribute name="attr6">6</Attribute>
        </AttributeOrder>
    </Implementation>

</CredentialStructure>